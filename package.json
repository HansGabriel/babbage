{
  "name": "babbage",
  "version": "0.0.1",
  "description": "Half-serious Math exercises cross-platform app to hopefully üôè help the lagging Math and science skills of the Filipino students.",
  "productName": "Babbage",
  "author": "Richard Michael Coo <rmcoo@cpu.edu.ph>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "start": "run-p -l 'dev:**'",
    "dev:frontend": "quasar dev --color",
    "dev:backend": "ts-node-dev -P tsconfig.backend.json --respawn src/server/index.ts --color"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "quasar": "^1.0.0",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@quasar/app": "^2.0.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@vue/composition-api": "^1.0.0-beta.22",
    "babel-eslint": "^10.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-typescript": "^1.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-unicorn": "^25.0.1",
    "eslint-plugin-vue": "^7.4.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
